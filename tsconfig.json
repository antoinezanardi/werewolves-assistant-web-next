{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "types": [
      "vitest/globals"
    ],
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
  },
  "ts-node": {
    "esm": true,
  },
}