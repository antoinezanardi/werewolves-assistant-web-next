{
  "name": "werewolves-assistant-web-next",
  "version": "1.26.0",
  "description": "Werewolves Assistant Web is a Nuxt Web App using the Werewolves Assistant API. It helps you, the game master, to manage your games of Werewolves Of Millers Hollow.",
  "readme": "README.md",
  "type": "module",
  "keywords": [
    "nuxt",
    "werewolves",
    "assistant",
    "web",
    "typescript"
  ],
  "homepage": "https://github.com/antoinezanardi/werewolves-assistant-web-next#readme",
  "bugs": {
    "url": "https://github.com/antoinezanardi/werewolves-assistant-web-next/issues",
    "email": "antoine.zanardi@epitech.eu"
  },
  "license": "MIT",
  "author": {
    "name": "Antoine Zanardi",
    "email": "antoine.zanardi@epitech.eu",
    "url": "https://antoinezanardi.fr"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antoinezanardi/werewolves-assistant-web-next.git"
  },
  "scripts": {
    "start": "npm run start:prod",
    "start:prod": "node .output/server/index.mjs",
    "prepare": "is-ci || husky",
    "build": "nuxt build",
    "dev": "nuxt dev --dotenv env/.env.development",
    "generate": "nuxt generate",
    "preview": "npm run build && nuxt preview --dotenv env/.env.production",
    "postinstall": "! ls -log -u node_modules/.bin/nuxt || nuxt prepare",
    "lint": "npx eslint --config eslint.config.js .",
    "lint:fix": "npx eslint --config eslint.config.js --fix .",
    "lint:staged:fix": "lint-staged -c config/lint-staged/.lintstagedrc.eslint.json",
    "lint:inspect-config": "npx @eslint/config-inspector",
    "prettier:gherkin": "prettier --config config/prettier/.prettierrc.cucumber.json --check **/*.feature",
    "prettier:gherkin:fix": "prettier --config config/prettier/.prettierrc.cucumber.json --write **/*.feature",
    "test:unit": "cross-env NODE_ENV=test vitest --config config/vitest/vitest.unit-config.ts",
    "test:unit:watch": "cross-env NODE_ENV=test vitest --watch --config config/vitest/vitest.unit-config.ts",
    "test:unit:cov": "cross-env NODE_ENV=test rimraf tests/unit/coverage && vitest --coverage --config config/vitest/vitest.unit-config.ts",
    "test:unit:staged": "lint-staged -c config/lint-staged/.lintstagedrc.unit-tests.json",
    "test:stryker": "cross-env NODE_ENV=test rimraf tests/stryker/coverage && stryker run config/stryker/stryker.conf.mjs",
    "test:stryker:force": "cross-env NODE_ENV=test rimraf tests/stryker/incremental.json tests/stryker/coverage && stryker run config/stryker/stryker.conf.mjs --force",
    "test:stryker:ci": "cross-env NODE_ENV=test rimraf tests/stryker/coverage && stryker run config/stryker/stryker-ci.conf.mjs",
    "test:cucumber:prepare": "playwright install --with-deps chromium",
    "test:cucumber": "cross-env NODE_OPTIONS='--import tsx/esm' cucumber-js --config config/cucumber/cucumber.json; EXIT_CODE_CUCUMBER=$?; npm run test:cucumber:html-report; exit $EXIT_CODE_CUCUMBER",
    "test:cucumber:shard-1": "cross-env NODE_OPTIONS='--import tsx/esm' cucumber-js --config config/cucumber/cucumber.json --tags @shard-1; EXIT_CODE_CUCUMBER=$?; npm run test:cucumber:html-report; exit $EXIT_CODE_CUCUMBER",
    "test:cucumber:shard-2": "cross-env NODE_OPTIONS='--import tsx/esm' cucumber-js --config config/cucumber/cucumber.json --tags @shard-2; EXIT_CODE_CUCUMBER=$?; npm run test:cucumber:html-report; exit $EXIT_CODE_CUCUMBER",
    "test:cucumber:shard-3": "cross-env NODE_OPTIONS='--import tsx/esm' cucumber-js --config config/cucumber/cucumber.json --tags @shard-3; EXIT_CODE_CUCUMBER=$?; npm run test:cucumber:html-report; exit $EXIT_CODE_CUCUMBER",
    "test:cucumber:shard-4": "cross-env NODE_OPTIONS='--import tsx/esm' cucumber-js --config config/cucumber/cucumber.json --tags @shard-4; EXIT_CODE_CUCUMBER=$?; npm run test:cucumber:html-report; exit $EXIT_CODE_CUCUMBER",
    "test:cucumber:skip-screenshots-comparison": "cross-env SKIP_SCREENSHOTS_COMPARISON_TESTS=true npm run test:cucumber",
    "test:cucumber:html-report": "tsx tests/acceptance/plugins/html-reporter/cucumber-html-reporter.ts",
    "docker:sandbox-api:start": "docker compose -f docker/werewolves-assistant-sandbox-api/docker-compose.yml up -d",
    "docker:sandbox-api:stop": "docker compose -f docker/werewolves-assistant-sandbox-api/docker-compose.yml down -v",
    "docker:sandbox-api:reset": "npm run docker:sandbox-api:stop && npm run docker:sandbox-api:start",
    "docker:production-example:start": "docker compose -f docker/werewolves-assistant-production-example/docker-compose.yml up -d",
    "docker:production-example:stop": "docker compose -f docker/werewolves-assistant-production-example/docker-compose.yml down",
    "docker:production-example:reset": "npm run docker:production-example:stop && npm run docker:production-example:start",
    "playwright:codegen": "npx playwright codegen http://localhost:3000",
    "script:create-branch": "scripts/create-git-branch.sh",
    "script:create-pull-request": "scripts/create-pull-request.sh",
    "script:check-acceptance-features-shard": "scripts/check-acceptance-features-shard.sh tests/acceptance",
    "gource": "gource --load-config config/gource/gource.config"
  },
  "devDependencies": {
    "@aksharahegde/nuxt-glow": "^1.1.2",
    "@chenfengyuan/vue-countdown": "^2.1.2",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@cucumber/cucumber": "^10.8.0",
    "@eslint/config-inspector": "^0.5.2",
    "@eslint/eslintrc": "^3.1.0",
    "@faker-js/faker": "^8.4.1",
    "@nuxt/image": "^1.7.0",
    "@nuxt/kit": "^3.12.4",
    "@nuxt/test-utils": "^3.14.0",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/i18n": "^8.3.3",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "@pinia/nuxt": "^0.5.3",
    "@pinia/testing": "^0.1.5",
    "@playwright/test": "^1.46.0",
    "@primevue/nuxt-module": "^4.0.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@stryker-mutator/api": "^8.4.0",
    "@stryker-mutator/core": "^8.4.0",
    "@stryker-mutator/typescript-checker": "^8.4.0",
    "@stryker-mutator/vitest-runner": "^8.4.0",
    "@stylistic/eslint-plugin": "^2.6.1",
    "@types/chart.js": "^2.9.41",
    "@types/eslint": "^9.6.0",
    "@types/howler": "^2.2.11",
    "@types/javascript-color-gradient": "^2.4.2",
    "@types/pixelmatch": "^5.2.6",
    "@types/pngjs": "^6.0.5",
    "@types/qs": "^6.9.15",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "@typescript-eslint/types": "^8.0.1",
    "@vitest/coverage-istanbul": "^2.0.5",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^10.11.0",
    "@vueuse/nuxt": "^10.11.0",
    "chart.js": "^4.4.3",
    "chartjs-plugin-datalabels": "^2.2.0",
    "circle-progress.vue": "^3.2.1",
    "class-transformer": "^0.5.1",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "cross-env": "^7.0.3",
    "cucumber-html-reporter": "^7.1.1",
    "eslint": "^9.8.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vue": "^9.27.0",
    "fuse.js": "^7.0.0",
    "happy-dom": "^14.12.3",
    "howler": "^2.2.4",
    "husky": "^9.1.4",
